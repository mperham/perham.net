<div class="AdminPlugins_legend">
  <span id="AdminPlugins_legend_active_msg_{$legendId}" class="icon-plugin-active"
   style="margin-right: 10px; vertical-align: top; display: none">
    {g->text text="up to date"}
  </span>

  <span id="AdminPlugins_legend_inactive_msg_{$legendId}" class="icon-plugin-inactive"
   style="margin-right: 10px; vertical-align: top; display: none">
    {g->text text="disabled"}
  </span>

  <span id="AdminPlugins_legend_unupgraded_msg_{$legendId}" class="icon-plugin-upgrade"
   style="margin-right: 10px; vertical-align: top; display: none">
    {g->text text="upgrade required"}
  </span>

  <span id="AdminPlugins_legend_uninstalled_msg_{$legendId}" class="icon-plugin-uninstall"
   style="margin-right: 10px; vertical-align: top; display: none">
    {g->text text="not installed"}
  </span>

  <span id="AdminPlugins_legend_incompatible_msg_{$legendId}" class="icon-plugin-incompatible"
   style="margin-right: 10px; vertical-align: top; display: none">
    {g->text text="incompatible"}
  </span>
</div>
